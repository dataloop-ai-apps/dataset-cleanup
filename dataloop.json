{
    "version": "0.0.1",
    "name": "dataset-cleanup",
    "displayName": "Dataset Cleanup",
    "description": "Provides cleanup on the dataset",
    "attributes": {
        "Provider": "Dataloop",
        "Category": "Element",
        "Element Type": "Studio"
    },
    "scope": "project",
    "components": {
        "panels": [
            {
                "name": "cleanup",
                "supportedSlots": [
                    {
                        "type": "datasetBrowser",
                        "configuration": {
                            "layout": {
                                "rightBarView": "default"
                            }
                        }
                    }
                ],
                "conditions": {}
            }
        ],
        "toolbars": [
            {
                "displayName": "Cleanup",
                "icon": "",
                "invoke": {
                    "type": "panel",
                    "namespace": "cleanup"
                },
                "location": "datasetBrowserTab"
            }
        ],
        "modules": [
            {
                "name": "cleanup",
                "entryPoint": "app.py",
                "className": "Runner",
                "initInputs": [],
                "config": {
                    "isCustomServer": true
                },
                "functions": [
                    {
                        "name": "run",
                        "output": [],
                        "input": []
                    }
                ]
            }
        ],
        "services": [
            {
                "name": "cleanup-runner",
                "displayName": "cleanup-runner",
                "moduleName": "cleanup",
                "panelNames": ["cleanup"],
                "initParams": {},
                "runtime": {
                    "podType": "regular-s",
                    "numReplicas": 1,
                    "concurrency": 10,
                    "runnerImage": "gcr.io/viewo-g/piper/agent/runner/cpu/main:latest",
                    "singleAgent": false,
                    "autoscaler": {
                        "type": "rabbitmq",
                        "minReplicas": 1,
                        "maxReplicas": 1,
                        "queueLength": 1
                    },
                    "preemptible": false,
                    "executionTimeout": 3600,
                    "drainTime": 600,
                    "onReset": "failed",
                    "runExecutionAsProcess": false
                },
                "maxAttempts": 3
            }
        ]
    }
}
